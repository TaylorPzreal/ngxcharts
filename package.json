{
  "name": "smart-chart",
  "version": "1.0.0-beta.0",
  "description": "Angular4 chart use d3.js",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "copy": "node tools/copy-package.js && node tools/cleanup.js",
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && tsc && ngc",
    "postbundle": "node tools/removecode.js",
    "build": "npm run bundle && npm run copy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaylorPzreal/smart-chart.git"
  },
  "keywords": [
    "angular4",
    "d3.js",
    "chart"
  ],
  "author": "TaylorPzreal <taylorpzreal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TaylorPzreal/smart-chart/issues"
  },
  "homepage": "https://github.com/TaylorPzreal/smart-chart#readme",
  "devDependencies": {
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@types/d3": "^4.11.0",
    "@types/node": "^8.0.46",
    "del": "^3.0.0",
    "node-sass": "^4.5.3",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^8.2.4",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rxjs": "^5.5.0",
    "typescript": "^2.5.3",
    "zone.js": "^0.8.18"
  },
  "dependencies": {
    "d3": "^4.11.0"
  }
}
